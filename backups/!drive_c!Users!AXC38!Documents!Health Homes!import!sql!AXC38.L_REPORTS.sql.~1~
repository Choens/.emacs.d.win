  ,REPORT_DATE varchar2(20)
  ,REPORT_PERIOD number
  ,BEGIN_DATE date
  ,END_DATE date
  ,CONSTRAINT L_REPORT_PLANS_PK PRIMARY KEY ( ID ) ENABLE
);

