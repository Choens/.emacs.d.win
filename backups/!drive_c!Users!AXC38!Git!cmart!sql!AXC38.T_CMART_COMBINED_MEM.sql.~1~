-- #############################################################################
-- AXC38.T_CMART_COMBINED_MEM
--
-- #############################################################################

create table AXC38.T_CMART_COMBINED_MEM
(
  ClientID varchar(10)
  ,CIN varchar(10)
  ,LastName varchar(25)
  ,FirstName varchar(25)
  ,DOB date
  ,TriggerDate date
  ,ProgramType varchar(25)
  ,AbleContact varchar(25)
  ,ContactDate varchar(25)
  ,AppropriateCM varchar(25)
  ,RefusedCM varchar(25)
  ,EnrolledCM varchar(25)
  ,EnrollCMDate date
  ,Intensity varchar(25)
  ,CountMail int
  ,CountPhone int
  ,CountPerson int
  ,CaseClosed varchar(25)
  ,ClosureDate date
  ,ReasonClosure varchar(50)
  ,CaseReopened varchar(25)
  ,DateReopened date
  ,ProgramType2 int
  ,AbleContact2 int
  ,AppropriateCM2 int
  ,RefusedCM2 int
  ,EnrolledCM2 int
  ,Intensity2 int
  ,CaseClosed2 int
  ,ReasonClosure2 int
  ,CaseReopened2 int
) ;


CREATE INDEX AXC38.T_CMART_COMBINED_MEM_CIN ON AXC38.T_CMART_COMBINED_MEM ("CIN") ;
